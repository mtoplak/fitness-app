    "@types/node": "^22.7.5",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.12.0",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^10.4.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
