    "test:watch": "jest --watch",
    "seed:classes": "tsx src/scripts/seedClasses.ts",
    "seed:users": "tsx src/scripts/seedUsers.ts",
    "seed:memberships": "tsx src/scripts/seedMemberships.ts",
    "seed:bookings": "tsx src/scripts/seedBookings.ts",
    "seed:packages": "tsx src/scripts/seedPackages.ts",
    "seed:trainers": "tsx src/scripts/seedTrainerProfiles.ts",
